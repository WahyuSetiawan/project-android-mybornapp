<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.apps.karama.mybornapp.MainMenu">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="?attr/colorPrimary"
        android:id="@+id/relativeLayout3">

        <ImageButton
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/setting"
            android:onClick="settingClick"
            android:id="@+id/imageButton10"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="MYBORN "
            android:id="@+id/textView14"
            android:textColor="#fcfbfb"
            android:singleLine="true"
            android:textSize="20dp"
            android:layout_alignBottom="@+id/imageButton10"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />
    </RelativeLayout>

    <ImageButton
        android:layout_width="82dp"
        android:layout_height="82dp"
        android:background="@drawable/msb"
        android:id="@+id/imageButton"
        android:onClick="onmsbClick"
        android:layout_marginBottom="39dp"
        android:layout_above="@+id/imageButton5"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="82dp"
        android:background="@drawable/hpl"
        android:id="@+id/imageButton2"
        android:onClick="onhplClick"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/konstraksi"
        android:id="@+id/imageButton5"
        android:onClick="onkonstraksiClick"
        android:layout_alignTop="@+id/imageButton2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/imageButton7"
        android:onClick="onkickClick"
        android:background="@drawable/kick"
        android:layout_marginTop="60dp"
        android:layout_below="@+id/textView6"
        android:layout_toRightOf="@+id/imageButton"
        android:layout_toEndOf="@+id/imageButton" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/a"
        android:id="@+id/imageButton9"
        android:onClick="onalarmClick"
        android:layout_alignTop="@+id/imageButton7"
        android:layout_toRightOf="@+id/imageButton5"
        android:layout_toEndOf="@+id/imageButton5" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="MYBORN "
        android:textColor="#010101"
        android:id="@+id/textView5"
        android:gravity="center_horizontal"
        android:textSize="20dp"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/imageButton5"
        android:layout_alignStart="@+id/imageButton5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Cyrcle Pregnancy"
        android:id="@+id/textView6"
        android:textColor="#db0364"
        android:textSize="15dp"
        android:layout_below="@+id/imageButton2"
        android:layout_centerHorizontal="true" />
    <Button
        android:layout_width="230dp"
        android:layout_height="wrap_content"
        android:text="View"
        android:id="@+id/btListvIEW"
        android:onClick="viewkick"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
